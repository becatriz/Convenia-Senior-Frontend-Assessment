{
  "name": "barcontrol",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "coverage": "vitest --environment jsdom --coverage",
    "build-only": "vite build",
    "prepare": "husky install",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "graphql-codegen",
    "format": "prettier --write .",
    "commit": "git-cz"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@meforma/vue-toaster": "^1.3.0",
    "@vue/apollo-composable": "^4.0.0-beta.1",
    "@vue/apollo-option": "^4.0.0-alpha.20",
    "axios": "^1.2.3",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "pinia": "^2.0.28",
    "pinia-plugin-persist": "^1.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "vue": "^3.2.45",
    "vue-cli-plugin-pinia": "^0.2.3",
    "vue-router": "^4.1.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/client-preset": "1.2.6",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-vue-apollo": "^3.3.7",
    "@miragejs/graphql": "^0.1.13",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-events": "^6.2.9",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.15",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.3.0",
    "commitizen": "^4.3.0",
    "cypress": "^12.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "event-emitter": "^0.3.5",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "miragejs": "^0.1.47",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
